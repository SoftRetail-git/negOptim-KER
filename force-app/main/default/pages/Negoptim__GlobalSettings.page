                <apex:actionFunction name="searchUserRole" action="{!searchUserRole}" reRender="userRoleSelectList, apexMessages" oncomplete="showListUR();">
                    <apex:param name="roleSearchTerm" assignTo="{!roleSearchTerm}" value=""/>
                </apex:actionFunction>
                <apex:actionFunction name="searchBU" action="{!searchBU}" reRender="buSelectList, apexMessages" oncomplete="showListBU();">
                    <apex:param name="buSearchTerm" assignTo="{!buSearchTerm}" value=""/>
                </apex:actionFunction>
            </apex:form>
            <script>
            var defaultToggleIndex;
            function decimalOnly(e){
                e.value = e.value.replace(/[^0-9\.]/g,'');
                if(e.value.split('.').length>2) e.value = e.value.replace(/\.+$/,"");
            };
            let focusedSelectList;
            function selectOption(selectedListId, defaultListId, inputClassName) {
                let list = document.getElementById(defaultListId);
                let selectedElement = [];
                for(let item of list.options) {
                    if(item.selected) {
                        selectedElement.push(item);
                    }
                }
                if (selectedElement.length > 0) {
                    let SelectedOptions = document.getElementById(selectedListId);
                    for(let item of selectedElement) {
                        item.remove();
                        item.selected = false;
                        SelectedOptions.appendChild(item);
                    }
                    let values =[];
                    for(let item of SelectedOptions.options) {
                        values.push(item.value);
                    }
                    document.querySelector('.' + inputClassName).value = values.join(',');
                }
            }
            function removeOption(selectedListId, defaultListId, inputClassName) {
                let SelectedKPI = document.getElementById(selectedListId);
                let selectedElement = [];
                for(let item of SelectedKPI.options) {
                    if(item.selected) {
                        selectedElement.push(item);
                    }
                }
                if (selectedElement.length > 0) {
                    let list = document.getElementById(defaultListId);
                    for(let item of selectedElement) {
                        item.remove();
                        item.selected = false;
                        list.appendChild(item);
                    }
                    let values =[];
                    for(let item of SelectedKPI.options) {
                        values.push(item.value);
                    }
                    document.querySelector('.' + inputClassName).value = values.join(',');
                }
            }
            function focusOn(who, selectedListId, defaultListId) {
                this.focusedSelectList = who;
                if (who == 'Old'){
                    let DefaultOptions = document.getElementById(defaultListId);
                    for(let item of DefaultOptions.options) {
                        item.selected = false;
                    }
                } else {
                    let SelectedOptions = document.getElementById(selectedListId);
                    for(let item of SelectedOptions.options) {
                        item.selected = false;
                    }
                }
            }
            function moveUp(selectedListId, inputClassName) {
                if (this.focusedSelectList == 'Old') {
                    let SelectedOptions = document.getElementById(selectedListId);
                    let length = SelectedOptions.options.length;
                    let options = [];
                    let selectedElement = [];
                    for(let item of SelectedOptions.options) {
                        if (item.selected) {
                            selectedElement.push(item);
                        }
                        options.push(item);
                    }
                    if (options.length == 0) {
                        return;
                    }
                    let oldIndexList = [];
                    for (let k = 0; k < length; k++) {
                        for (let l = 0; l < selectedElement.length; l++) {
                            if (options[k].value == selectedElement[l].value) {
                                oldIndexList[l] = k;
                                break;
                            }
                        }
                    }
                    oldIndexList.sort(function (a, b) {
                        return a - b
                    });
                    for (let t = 0; t < oldIndexList.length; t++) {
                        options = array_move_one_item(options, oldIndexList[t], oldIndexList[t] - 1);
                    }
                    SelectedOptions.innerHTML = "";
                    let values = [];
                    for (let index=0; index < length; index++) {
                        SelectedOptions.appendChild(options[index]);
                        values.push(options[index].value);
                    }
                    document.querySelector('.' + inputClassName).value = values.join(',');
                }
            }
            function moveDown(selectedListId, inputClassName) {
                if (this.focusedSelectList == 'Old') {
                    let SelectedOptions = document.getElementById(selectedListId);
                    let length = SelectedOptions.options.length;
                    let options = [];
                    let selectedElement = [];
                    for(let item of SelectedOptions.options) {
                        if (item.selected) {
                            selectedElement.push(item);
                        }
                        options.push(item);
                    }
                    if (options.length == 0) {
                        return;
                    }
                    let oldIndexList = [];
                    for (let k = 0; k < options.length; k++) {
                        for (let l = 0; l < selectedElement.length; l++) {
                            if (options[k].value == selectedElement[l].value) {
                                oldIndexList[l] = k;
                                break;
                            }
                        }
                    }
                    oldIndexList.sort(function (a, b) {
                        return b - a;
                    });
                    for (let t = 0; t < oldIndexList.length; t++) {
                        options = array_move_one_item(options, oldIndexList[t], oldIndexList[t] + 1);
                    }
                    let values = [];
                    SelectedOptions.innerHTML = "";
                    for (let index=0; index < length; index++) {
                        SelectedOptions.appendChild(options[index]);
                        values.push(options[index].value);
                    }
                    document.querySelector('.' + inputClassName).value = values.join(',');
                }
            }
            function array_move_one_item(arr, old_index, new_index){
                if (new_index < 0) {
                    new_index = 0;
                }
                if (new_index > arr.length) {
                    new_index = arr.length;
                }
                arr.splice(new_index, 0, arr.splice(old_index, 1)[0]);
                return arr;
            }
            function setValueUserRoleValue(value, label){
                let elemt = document.querySelector('.UR' + defaultToggleIndex);
                if (elemt) {
                    elemt.parentElement.children[0].value = label;
                    elemt = elemt.children[0];
                    if (elemt) {
                        elemt.value = value;
                    }
                }
                hideAllLists();
            }
            function setValueBuValue(value, label){
                let elemt = document.querySelector('.BU' + defaultToggleIndex);
                if (elemt) {
                    elemt.parentElement.children[0].value = label;
                    elemt = elemt.children[0];
                    if (elemt) {
                        elemt.value = value;
                    }
                }
                hideAllLists();
            }
            function hideAllLists() {
                let elems = document.querySelectorAll('[class*="slds-combobox-toggle-"].slds-is-open , [class*="slds-combobox-toggle-"].slds-is-open');
                for (let index = 0; elems && index < elems.length; index++) {
                    elems[index].classList.remove('slds-is-open');
                }
            }
            function showListUR(){
                hideAllLists();
                let elm = document.querySelector('.slds-combobox-role-toggle-' + defaultToggleIndex);
                if(elm) {
                    elm.classList.add('slds-is-open');
                }
            }
            function showListBU(){
                hideAllLists();
                let elems = document.querySelectorAll('[class*="slds-combobox-toggle-"].slds-is-open');
                for (let index = 0; elems && index < elems.length; index++) {
                    elems[index].classList.remove('slds-is-open');
                }
                let elm = document.querySelector('.slds-combobox-bu-toggle-' + defaultToggleIndex);
                if (elm) {
                    elm.classList.add('slds-is-open');
                }
            }
            function setvalue(element) {
                if (element && !element.value) {
                    element.nextElementSibling.children[0].value = '';
                }
            }
            </script>
